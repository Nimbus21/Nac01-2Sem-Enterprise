@model Aluno

@{
    ViewData["Title"] = "Editar";
}

<h1>Editar</h1>

<form asp-action="editar" asp-controller="aluno">
    <input asp-for="Codigo" />
    <partial name="_Form" />
    <input type="submit" value="Editar" class="btn btn-primary" />
    <a asp-action="index" class="btn btn-dark">Cancelar</a>
</form>